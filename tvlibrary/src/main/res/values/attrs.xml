<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DoctorRecycleView">
        <!--<attr name="type" format="string"></attr>-->
        <attr name="column" format="integer" />
        <attr name="nextFocusLeft" format="reference" />
        <attr name="nextFocusUp" format="reference" />
        <attr name="nextFocusRight" format="reference" />
        <attr name="nextFocusDown" format="reference" />
        <attr name="type" >
            <enum name="list_v" value="1"/>
            <enum name="list_h" value="2"/>
            <enum name="list_grid" value="3"/>
        </attr>
        <attr name="oldView">
            <enum name="yes" value="1" />
            <enum name="no" value="0" />
        </attr>
        <attr name="can_loadmore">
            <enum name="yes" value="1" />
            <enum name="no" value="0" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ClubItemView">
        <attr name="name" format="string"></attr>
        <attr name="content" format="string"></attr>
        <attr name="layout_bg" format="reference"></attr>
    </declare-styleable>
</resources>
